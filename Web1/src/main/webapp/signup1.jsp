<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
</head>
<body>
	<table align="center">
		<tr>
			<td><h1
					style="color: #6A24FE; font-size: 40px; margin-bottom: 20px">Login</h1></td>
		</tr>
		<tr>
			<td><input type="text" name="Email" placeholder="Email"
				style="width: 300px; height: 40px; opacity: 0.7;"></td>
		</tr>
		<tr>
			<td><input type="password" name="user_Password"
				placeholder="Password"
				style="width: 300px; height: 40px; opacity: 0.7;"></td>
		</tr>
		<tr>
			<td><input type="checkbox" name="save_Login"
				style="background-color: #6A24FE;"> 아이디 저장하기</td>
		</tr>
		<tr>
			<td align="center"><input type="submit" value="로그인"
				style="background-color: #6A24FE; color: white; width: 300px; height: 40px; border-radius: 10px; border: none"></td>
		</tr>
	</table>


</body>
</html>